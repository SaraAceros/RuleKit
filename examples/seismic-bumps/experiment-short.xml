<?xml version="1.0"?>
<experiment>
	<parameter_sets>
		<parameter_set name="mincov=5, RSS">
			<param name="min_rule_covered">5</param>
			<param name="induction_measure">RSS</param>
			<param name="pruning_measure">RSS</param>
			<param name="voting_measure">RSS</param>
		</parameter_set>
	</parameter_sets>

	<datasets>
		<dataset>
			<label>class</label>
			<out_directory>./results</out_directory>		
    
			<training>  
				<report_file>training-log.txt</report_file>      
				 
				<train>
					 <in_file>../examples/seismic-bumps/cv-splits/seismic-bumps-train-9.arff</in_file>               		
					 <model_file>seismic-bumps-9.mdl</model_file>  
				 </train>
			</training>
    
			<prediction>
				<performance_file>performance.csv</performance_file>  
				
				<predict>
					<model_file>seismic-bumps-9.mdl</model_file>      	
					<test_file>../examples/seismic-bumps/cv-splits/seismic-bumps-test-9.arff</test_file>            			
					<predictions_file>seismic-bumps-pred-9.arff</predictions_file>   	
				</predict>
			</prediction>
    
		</dataset>
  	</datasets>
</experiment>